<img alt="Unsplash image random" loading="lazy" id="unsplashImg"  class="unsplash">
<script>

    let getImg = 'https://api.unsplash.com/photos/random?client_id=g1OiNzLO12axZyrJPqglbScQy2vbpD6BtcqsDIpCcIo&orientation=portrait&query=shoes';
    let imageElement = document.querySelector("#unsplashImg"); 
    fetch(getImg)
      .then((response) => response.json())
      .then((jsonData) => {
        imageElement.src = jsonData.urls.full;    
      });
        $(document).ready(function(){
        $(".unsplash").fadeIn(3000);
        console.log('Checking');
       })

  </script>